#include "lists.h"
/**
 * get_nodeint_at_index - The nth node of a listint
 * @head: Singly linked list
 * @index: Integer
 * Return: Singly Linked list
 */
listint_t *get_nodeint_at_index(listint_t *head, unsigned int index)
{
	listint_t *l = head, *o;
	unsigned int h;

	if (head == NULL)
		return (NULL);

	for (h = 0; h < index; h++)
	{
		if (l->next == NULL)
			return (NULL);
		o = l->next;
		l = o;
	}
	return (l);
}
